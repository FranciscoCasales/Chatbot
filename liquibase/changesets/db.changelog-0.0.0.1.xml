<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
    <changeSet author="sergio" id="20190321-01">
        <createTable tableName="contacts">
            <column name="id" type="int">
                <constraints nullable="true" primaryKey="true" unique="true"/>
            </column>
            <column name="ds_name" type="VARCHAR(20)">
                <constraints nullable="true" primaryKey="false" unique="false"/>
            </column>
            <column name="ds_lastname" type="VARCHAR(20)">
                <constraints nullable="true" primaryKey="false" unique="false"/>
            </column>
            <column name="phone" type="VARCHAR(10)">
                <constraints nullable="true" primaryKey="false" unique="false"/>
            </column>
            <column name="email" type="VARCHAR(50)">
                <constraints nullable="true" primaryKey="false" unique="false"/>
            </column>
            <column name="id_myaxity" type="int">
                <constraints nullable="true" primaryKey="false" unique="true"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog> 